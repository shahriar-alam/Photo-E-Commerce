@model IEnumerable<ECEntity.Transaction>

@{
    Layout = null;
}

<!DOCTYPE html>

<!DOCTYPE html>

<html>

<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewProfile</title>
    <link rel="stylesheet" type="text/css" href="~/css/Homedesign.css">
    <link rel="stylesheet" type="text/css" href="~/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/css/test.css">
    <link rel="stylesheet" type="text/css" href="~/css/header.css">
    <link rel="stylesheet" type="text/css" href="~/css/menu.css">
    <link rel="stylesheet" type="text/css" href="~/css/footer.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

</head>

<body>

    <header class="header">
        <div>
            <nav>
                <ul class="header_ul">
                    <li><a href="@Url.Action("Home", "Home")"><i class="fa fa-home" aria-hidden="true" style="color: white"></i></a></li>
                    <li><a href="@Url.Action("DownloadHistory", "Dashboard")"><i class="fa fa-shopping-cart" aria-hidden="true" style="color: white"></i></a></li>

                    <li>
                        <a href="@Url.Action("ViewProfile", "Dashboard")"><i class="fa fa-tachometer" aria-hidden="true" style="color: white"></i></a>
                    </li>
                    <li><a href="@Url.Action("Chart", "Dashboard")"><i class="fa fa-bar-chart" aria-hidden="true" style="color: white"></i></a></li>
                    <li><a href="@Url.Action("Logout", "Dashboard")"><i class="fa fa-sign-out" aria-hidden="true" style="color: white"></i></a></li>
                </ul>
            </nav>
        </div>
    </header>

    <menu class="home">
        <div>
            <nav class="bar">
                <ul></ul>
                <ul>
                    <li><font color="#444141" face="Impact" style="margin:15" size="4"></font></li>
                    <li class="menu_font">
                        <a href="@Url.Action("Home2", "Home", new { tags = "Nature" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Nature</u></font></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Home2", "Home", new { tags = "Digital Art" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Digital Art</u></font></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Home2", "Home", new { tags = "Film & Animation" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Film & Animation</u></font></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Home2", "Home", new { tags = "Design" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Design</u></font></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Home2", "Home", new { tags = "Cartoon" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Cartoon</u></font></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Home2", "Home", new { tags = "Anime" } )"><font color="#444141" face="Impact" style="margin:15" size="3"><u>Anime</u></font></a>
                    </li>
                    <hr>
                </ul>
            </nav>
        </div>
    </menu>
    <div class="pages">
        <ul>
            <li class="menu_font">
                <a href="index.html"><font color="black" face="Impact" style="margin:15" size="3">Reports</font> <i class="fa fa-angle-down" aria-hidden="true"></i></a>
                <ul class="dropdowns">
                    <li><a href="@Url.Action("Chart", "Dashboard", new { sort = "Date Ascending" } )"><font class="fnt">Date Ascending</font></a></li>

                    <li><a href="@Url.Action("Chart", "Dashboard", new { sort = "Date Descending" } )"><font class="fnt">Date Descending</font></a></li>

                    <li><a href="@Url.Action("Chart", "Dashboard", new { sort = "Price Ascending" } )"><font class="fnt">Price Ascending</font></a></li>

                    <li><a href="@Url.Action("Chart", "Dashboard", new { sort = "Price Descending" } )"><font class="fnt">Price Descending</font></a></li>

                    <li><a href="@Url.Action("Chart", "Dashboard", new { sort = "All" } )"><font class="fnt">All</font></a></li>
                </ul>
            </li>

        </ul>
    </div>
    <table class="table" id="" width="600px" height="100px" border="0" style="position: relative; bottom: 50">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.sellerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.buyerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransactionDate)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.sellerId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.buyerId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ItemId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ItemPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TransactionDate)
                </td>
                
            </tr>
        }

    </table>
</body>
</html>
